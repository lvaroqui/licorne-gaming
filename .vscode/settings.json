{
  "emmet.includeLanguages": {
    "rust": "html",
    "*.rs": "html"
  },
  "tailwindCSS.includeLanguages": {
    "rust": "html",
    "*.rs": "html"
  },
  "files.associations": {
    "*.rs": "rust"
  },
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "on",
    "strings": true
  },
  "css.validate": false,
  "rust-analyzer.server.extraEnv": {
    "CARGO_TARGET_DIR": "target/analyzer"
  },
  "rust-analyzer.check.extraArgs": [
    "--target-dir=target/analyzer"
  ]
}